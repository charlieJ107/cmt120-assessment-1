{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/js/debug.js",
            "request": "launch",
            "localRoot": "${workspaceFolder}/js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
    
        
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/python",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Test Framework",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/python",
            "program": "${workspaceFolder}/python/testingFramework_4.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "template.py"
            ],
        }
    ]
}